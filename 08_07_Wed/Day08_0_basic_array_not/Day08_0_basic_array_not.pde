

void setup() {
  size(250, 100);
  smooth();
  noStroke();
}

void draw() {
  background(0);

  fill(255, 0, 255);
  ellipse(30, 50, 10, 10);
  fill(255, 50, 255);
  ellipse(60, 50, 10, 10);
  fill(255, 100, 255);
  ellipse(90, 50, 10, 10);
  fill(255, 150, 255);
  ellipse(120, 50, 10, 10);
  fill(255, 200, 255);
  ellipse(150, 50, 10, 10);
  fill(255, 250, 255);
  ellipse(180, 50, 10, 10);
}





